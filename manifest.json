{
  "name": "Multiple Screens Application Example",
  "version": "1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [ "background.js" ],
      "persistent": false
    }
  },
  "permissions": [
    "webview", "alwaysOnTopWindows", "fullscreen", "overrideEscFullscreen"
  ],
  "kiosk_enabled": true
}